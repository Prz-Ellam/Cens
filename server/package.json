{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend for Cens Application",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "clean": "rimraf dist",
    "countries": "ts-node -r tsconfig-paths/register database/seeder/countries.ts",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/database.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/config/database.ts",
    "migration:create": "npm run typeorm migration:create",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "cross-env NODE_ENV=testing jest --silent tests/*",
    "transpile": "tsc && tsc-alias",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "keywords": [
    "Cens"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/convict": "^6.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "20.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "rimraf": "5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.7",
    "tsconfig-paths": "4.2.0",
    "typeorm-extension": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-xss-sanitizer": "^1.1.6",
    "file-type": "16.5.3",
    "helmet": "^7.0.0",
    "i18n-iso-countries": "^7.6.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "request_trimmer": "^1.0.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-shield": "^1.0.1",
    "zod": "^3.22.2"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "npm run lint:fix"
    ]
  }
}
